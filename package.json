{
  "name": "ournet.web.weather",
  "version": "7.0.0",
  "description": "Ournet weather-web-app",
  "main": "./lib/index.js",
  "watch": {
    "start": {
      "patterns": [
        "lib"
      ],
      "extensions": "js",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2000
    }
  },
  "scripts": {
    "dev": "npm run tsc && npm run start",
    "start": "node ./lib/app.js",
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "build": "npm run clean && npm run tsc",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib",
    "clean-test-code": "rimraf ./lib/**/*.test.js",
    "watch": "npm-watch start"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ournet/ournet.web.weather.git"
  },
  "keywords": [
    "ournet",
    "weather",
    "ournet.web.weather"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ournet/ournet.web.weather/issues"
  },
  "homepage": "https://github.com/ournet/ournet.web.weather#readme",
  "devDependencies": {
    "@types/boom": "^7.2.0",
    "@types/express": "^4.11.1",
    "@types/i18n": "^0.8.3",
    "@types/lru-cache": "^4.1.0",
    "@types/moment-timezone": "^0.5.6",
    "@types/node": "latest",
    "@types/node-fetch": "^2.1.1",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "ava": "^0.23.0",
    "npm-watch": "^0.3.0",
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "dependencies": {
    "@ournet/places-domain": "^0.1.4",
    "@ournet/weather-domain": "^0.1.4",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "body-parser": "^1.18.3",
    "boom": "^7.2.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "i18n": "^0.8.3",
    "lru-cache": "^4.1.3",
    "moment-timezone": "^0.5.17",
    "ms": "^2.1.1",
    "node-fetch": "^2.1.2",
    "ournet.links": "^0.4.2",
    "ournet.logger": "^0.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  }
}
